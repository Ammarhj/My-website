الجسم {
 عائلة الخط: Arial، sans-serif ؛ 
         line-height: 1.6;     
 الهامش: 0؛ 
 الحشو: 0؛ 
 اللون: #333؛ 
}

رأس {
 خلفية: #007BFF; 
 اللون: أبيض. 
 الحشو: 1rem 0; 
    محاذاة النص: المركز؛    
}

رأس ناف {
   اللون: أبيض.   
   تزيين النص: لا شيء؛   
    الهامش: 0.15px؛    
    حجم الخط: 1.2 ريم.    
}

رأس نافا: تحوم {
   تزيين النص: underline;   
}

الرئيسية {
   الحشو: 20px.   
   محاذاة النص: المركز؛   
}

تذييل {
   الخلفية: #333؛   
   اللون: أبيض.   
   محاذاة النص: المركز؛   
    الحشو: 10px 0;    
    هامش أعلى: 20px.    
}

شكل {
     العرض الأقصى: 400px.     
     الهامش: السيارات؛     
     محاذاة النص: يسار؛     
}

شكل التسمية {
 عرض: كتلة; 
     هامش أعلى: 10px.     
}

إدخال النموذج، شكل منطقة النص، زر النموذج {.
     العرض: 100%؛     
     الحشو: 10px.     
     هامش أعلى: 5px.     
     الحدود: 1px الصلبة #ddd;     
     الحدود-راديوس: 5px;     
}

نموذج زر {
     خلفية: #007BFF;     
     اللون: أبيض.     
     الحدود: لا شيء؛     
     المؤشر: مؤشر؛     
}

   نموذج الزر: تحوم.     {
     الخلفية: #0056b3؛     
}
