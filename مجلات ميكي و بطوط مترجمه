  <الاسم الفوقي="ViewPort" content="العرض=عرض على الجهاز, النطاق الأولي=1.0">  
     <title>التواصل</title> 
 <الرابط rel="Stylesheet" href="style.css"> 
<الرأس>
<الجسم>
 <الرأس> 
         <h1>التواصل معنا</h1> 
 <ناف> 
             <a href="index.html">الرئيسية</a> 
             <a href="files.html">الملفات</a> 
             <a href="contact.html">التواصل</a> 
 </ناف> 
  </الرأس>  
 <الرئيسية> 
  القسم> 
             <h2>نموذج التواصل</h2> 
 <شكل action="https://formspree.io/f/{your-email}"طريقة="POST"> 
                 <label for="name">الاسم:</label> 
 <نوع الإدخال="نص" id="اسم"="اسم" مطلوب> 
                
                 <label for="email">البريد الإلكتروني:</label> 
  <نوع الإدخال="البريد الإلكتروني" id="البريد الإلكتروني" (الاسم ="البريد الإلكتروني" مطلوب>)  
                
                 <label for="message">الرسالة:</label> 
 <textarea id="رسالة" name="رسالة" الصفوف="5" مطلوب></textarea> 
                
                  <button type="submit">إرسال</button>  
   </شكل>   
